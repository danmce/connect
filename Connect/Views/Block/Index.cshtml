@{
    ViewBag.Title = "Block";
}

<div class="row">
    <div class="large-12 columns">
        <h1>Block</h1>
    </div>
</div>
<div class="row">
    <div class="large-12 columns">
        <canvas id="block" width="500" height="200" class="ui-widget-content">
            <div class="canvas">
                <div class="block"></div>
                <div class="block block-alternate"></div>
            </div>
        </canvas>
    </div>
</div>

@section Scripts{
    <script>
        CONNECT.drawBlocks('block');
    </script>
}